; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dialogue Helper"
config/version="1.2"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0.145098, 0.145098, 0.145098, 1)
boot_splash/show_image=false
config/icon="res://icon.png"
boot_splash/stretch/aspect="expand"

[autoload]

Handle="*res://Scripts/Handle.gd"
IFileHandler="*res://Scripts/IFileHandler.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1100
window/size/viewport_height=700
window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="Dialogue Helper"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[gui]

theme/custom="res://Theme.tres"

[network]

limits/debugger/max_chars_per_second=999999999
limits/debugger/max_queued_messages=999999999

[rendering]

rendering_device/pipeline_cache/enable=false
rendering_device/driver.windows="d3d12"
shader_compiler/shader_cache/enabled=false
environment/defaults/default_clear_color=Color(0.160784, 0.172549, 0.254902, 1)
